<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.wh.mode.mapper.MovieMapper">

    <resultMap id="BaseResultMap" type="cn.wh.mode.pojo.Movie">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="movieName" column="movie_name" jdbcType="VARCHAR"/>
            <result property="movieUrl" column="movie_url" jdbcType="VARCHAR"/>
            <result property="movieSort" column="movie_sort" jdbcType="VARCHAR"/>
            <result property="movieHide" column="movie_hide" jdbcType="INTEGER"/>
            <result property="movieDelete" column="movie_delete" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,movie_name,movie_url,
        movie_sort,movie_hide,movie_delete
    </sql>
</mapper>
